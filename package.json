{
  "name": "phungible",
  "version": "0.8.1",
  "description": "Phungible budget app",
  "engines": {
    "node": ">=6.10.3"
  },
  "main": "src/server/index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "export NODE_ENV=\"production\" && gulp build:prod",
    "clean": "rm -Rf build",
    "explore-bundle": "source-map-explorer ./build/client/js/app.js",
    "server": "http-server ./build/client/",
    "start": "npm run server & gulp",
    "start:prod": "npm run build:prod && npm run server",
    "test": "npm run test:unit && npm run test:lint",
    "test:coverage": "nyc -e '.ts' --r html -r lcov -r text npm run test:unit",
    "test:unit": "export TS_NODE_PROJECT=src/client && TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha --compilers ts:ts-node/register,tsx:ts-node/register src/client/**/*.test.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:lint": "tslint --project tslint.json src/**/*.ts*"
  },
  "author": "David Granado",
  "license": "ISC",
  "devDependencies": {
    "@types/accounting": "^0.4.1",
    "@types/chai": "^4.0.4",
    "@types/classnames": "^2.2.3",
    "@types/material-ui": "^0.18.2",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.31",
    "@types/pouchdb": "^6.3.0",
    "@types/react": "^16.0.7",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "^15.5.5",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-cache-bust": "^1.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.2.2",
    "gulp-webpack": "^1.5.0",
    "http-server": "^0.10.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "source-map-explorer": "^1.5.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.1.0",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.2.5",
    "material-ui": "^0.19.2",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.3",
    "moment": "^2.17.1",
    "moment-recur": "^1.0.5",
    "navigo": "^5.2.0",
    "offline-plugin": "^4.8.4",
    "pouchdb": "^6.3.4",
    "pouchdb-authentication": "^0.5.5",
    "react": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-transition-group": "^1.2.0",
    "recharts": "^1.0.0-apha.5",
    "secure-ls": "^1.1.0",
    "serializr": "^1.1.13",
    "whatwg-fetch": "^2.0.3"
  }
}
