{
  "name": "phungible",
  "version": "1.0.1",
  "description": "Phungible budget app",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "src/server/index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "export NODE_ENV=\"production\" && gulp build:prod",
    "clean": "rm -Rf build",
    "explore-bundle": "source-map-explorer ./build/client/js/app.js",
    "server": "http-server ./build/client/",
    "start": "npm run server & gulp",
    "start:prod": "npm run build:prod && npm run server",
    "test": "npm run test:unit && npm run test:lint",
    "test:coverage": "nyc -e '.ts' --r html -r lcov -r text npm run test:unit",
    "test:unit": "export TS_NODE_PROJECT=src/client && TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha --compilers ts:ts-node/register,tsx:ts-node/register src/client/**/*.test.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:lint": "tslint --project tslint.json src/**/*.ts*"
  },
  "author": "David Granado",
  "license": "ISC",
  "devDependencies": {
    "@types/accounting": "^0.4.1",
    "@types/chai": "^4.1.2",
    "@types/classnames": "^2.2.3",
    "@types/localforage": "0.0.34",
    "@types/material-ui": "^0.20.5",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.5.10",
    "@types/react": "^16.0.36",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "^16.0.3",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-cache-bust": "^1.3.0",
    "gulp-sequence": "^0.4.6",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.2.4",
    "gulp-webpack": "^1.5.0",
    "http-server": "^0.10.0",
    "mocha": "^3.5.3",
    "nyc": "^11.4.1",
    "source-map-explorer": "^1.5.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^3.4.0",
    "ts-node": "^3.1.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.2.5",
    "localforage": "^1.5.6",
    "localforage-cordovasqlitedriver": "^1.7.0",
    "material-ui": "^0.20.0",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.1",
    "mobx-utils": "^3.2.2",
    "moment": "^2.20.1",
    "moment-recur": "^1.0.5",
    "navigo": "^5.3.3",
    "offline-plugin": "^4.9.0",
    "react": "^16.2.0",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-transition-group": "^1.2.1",
    "recharts": "^1.0.0-beta.10",
    "serializr": "^1.1.14",
    "whatwg-fetch": "^2.0.3"
  }
}
