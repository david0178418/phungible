{
  "name": "phungible",
  "version": "0.1.10",
  "description": "Phungible budget app",
  "engines": {
    "node": ">=6.10.0"
  },
  "main": "src/server/index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "gulp build:prod",
    "clean": "rm -Rf build",
    "server:node": "node ./build/server/index.js",
    "server:static": "http-server ./build/client/",
    "start": "npm run server:static | gulp",
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "export TS_NODE_PROJECT=src/client && mocha --compilers ts:ts-node/register,tsx:ts-node/register src/client/**/*.test.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:lint": "tslint --project tslint.json src/**/*.ts*"
  },
  "author": "David Granado",
  "license": "ISC",
  "devDependencies": {
    "@types/accounting": "^0.3.31",
    "@types/chai": "^3.5.2",
    "@types/classnames": "^2.2.0",
    "@types/hapi": "^16.0.0",
    "@types/inert": "^4.0.0",
    "@types/lodash": "^4.14.64",
    "@types/material-ui": "^0.17.1",
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.0.12",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^15.5.0",
    "@types/react-tap-event-plugin": "^0.0.30",
    "awesome-typescript-loader": "^3.0.7",
    "chai": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-cache-bust": "^1.1.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-sequence": "^0.4.6",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.1.5",
    "gulp-webpack": "^1.5.0",
    "http-server": "^0.10.0",
    "mocha": "^3.3.0",
    "source-map-loader": "^0.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.0.0",
    "typescript": "^2.2.1",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.2.5",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.0",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "moment": "^2.17.1",
    "moment-recur": "^1.0.5",
    "navigo": "^4.6.0",
    "offline-plugin": "^4.7.0",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^1.1.2",
    "recharts": "^1.0.0-alpha.0",
    "secure-ls": "^1.1.0",
    "serializr": "^1.1.9"
  }
}
