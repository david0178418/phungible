{
	"name": "phungible",
	"version": "0.0.61",
	"author": "David Granado",
	"homepage": "https://phungible.com/",
	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --config webpack/webpack.config.js",
		"fn:serve": "npm run fn:build && GOOGLE_APPLICATION_CREDENTIALS='./src/functions/phungible-key.json' firebase emulators:start --only functions",
		"fn:build": "tsc --project src/functions/tsconfig.json",
		"fn:deploy": "firebase deploy --only functions",
		"build:prod": "NODE_ENV=production webpack --config webpack/webpack.config.js",
		"lint": "eslint --ext .ts,tsx,.js,.jsx src/",
		"lint:fix": "eslint --fix --ext .ts,tsx,.js,.jsx src/",
		"release": "npm run build:prod && npm run release:hosting",
		"release:hosting": "firebase deploy --only hosting"
	},
	"private": true,
	"dependencies": {
		"@ionic/react": "^5.0.7",
		"@ionic/react-router": "^5.0.7",
		"classnames": "^2.2.6",
		"currency.js": "^1.2.2",
		"date-fns": "^2.12.0",
		"eslint": "^6.8.0",
		"fast-deep-equal": "^3.1.1",
		"firebase": "^7.14.2",
		"frappe-charts": "^1.3.0",
		"ionicons": "^5.0.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.1.2",
		"react-use": "^14.2.0",
		"recharts": "^1.8.5",
		"rrule": "^2.6.4",
		"rxjs": "^6.5.5",
		"ts-key-enum": "^2.0.2",
		"tslib": "^1.11.1",
		"workbox-window": "^5.1.3"
	},
	"devDependencies": {
		"@types/classnames": "^2.2.10",
		"@types/node": "^13.13.2",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.6",
		"@types/react-router-dom": "^5.1.4",
		"@types/recharts": "^1.8.10",
		"@types/rrule": "^2.2.9",
		"@types/webpack": "^4.41.12",
		"@types/workbox-webpack-plugin": "^5.1.0",
		"@types/workbox-window": "^4.3.3",
		"clean-webpack-plugin": "^3.0.0",
		"concurrently": "^5.1.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.5.3",
		"eslint-plugin-react-app": "^6.2.2",
		"eslint-plugin-react-hooks": "^3.0.0",
		"file-loader": "^6.0.0",
		"firebase-tools": "^8.1.1",
		"html-webpack-plugin": "^4.2.0",
		"ionic": "^5.4.16",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.2.0",
		"ts-loader": "^7.0.1",
		"ts-node": "^8.9.0",
		"typescript": "^3.8.3",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2",
		"workbox-webpack-plugin": "^5.1.3"
	},
	"description": "Phungible: Simple budgeting"
}
