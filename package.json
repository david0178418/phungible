{
  "name": "phungible",
  "version": "0.7.4",
  "description": "Phungible budget app",
  "engines": {
    "node": ">=6.10.3"
  },
  "main": "src/server/index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "export NODE_ENV=\"production\" && gulp build:prod",
    "clean": "rm -Rf build",
    "server": "http-server ./build/client/",
    "start": "npm run server & gulp",
    "start:prod": "npm run build:prod && npm run server",
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "export TS_NODE_PROJECT=src/client && mocha --compilers ts:ts-node/register,tsx:ts-node/register src/client/**/*.test.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:lint": "tslint --project tslint.json src/**/*.ts*"
  },
  "author": "David Granado",
  "license": "ISC",
  "devDependencies": {
    "@types/accounting": "^0.3.31",
    "@types/chai": "^4.0.4",
    "@types/classnames": "^2.2.3",
    "@types/material-ui": "^0.18.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.24",
    "@types/pouchdb": "^6.3.0",
    "@types/react": "15.x.x",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "^15.5.4",
    "chai": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-cache-bust": "^1.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.2.2",
    "gulp-webpack": "^1.5.0",
    "http-server": "^0.10.0",
    "mocha": "^3.3.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.4",
    "ts-node": "^3.1.0",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.1",
    "webpack": "3.x.x"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.2.5",
    "material-ui": "^0.19.0",
    "mobx": "^3.x.x",
    "mobx-react": "4.x.x",
    "moment": "^2.17.1",
    "moment-recur": "^1.0.5",
    "navigo": "^5.2.0",
    "offline-plugin": "^4.7.0",
    "react": "15.x.x",
    "pouchdb": "^6.3.4",
    "pouchdb-authentication": "^0.5.5",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-transition-group": "^1.2.0",
    "recharts": "^1.0.0-alpha.4",
    "secure-ls": "^1.1.0",
    "serializr": "^1.1.12",
    "whatwg-fetch": "^2.0.3"
  }
}
