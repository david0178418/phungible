{
	"name": "phungible",
	"version": "0.0.66",
	"author": "David Granado",
	"homepage": "https://phungible.com/",
	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --config webpack/webpack.config.js",
		"fn:serve": "npm run fn:build && GOOGLE_APPLICATION_CREDENTIALS='./src/functions/phungible-key.json' firebase emulators:start --only functions",
		"fn:build": "tsc --project src/functions/tsconfig.json",
		"fn:deploy": "firebase deploy --only functions",
		"build:prod": "NODE_ENV=production webpack --config webpack/webpack.config.js",
		"lint": "eslint --ext .ts,tsx,.js,.jsx src/",
		"lint:fix": "eslint --fix --ext .ts,tsx,.js,.jsx src/",
		"release": "npm run build:prod && npm run release:hosting",
		"release:hosting": "firebase deploy --only hosting"
	},
	"private": true,
	"dependencies": {
		"@ionic/react": "^5.5.2",
		"@ionic/react-router": "^5.5.2",
		"classnames": "^2.2.6",
		"currency.js": "^1.2.2",
		"date-fns": "^2.16.1",
		"eslint": "^6.8.0",
		"fast-deep-equal": "^3.1.3",
		"firebase": "^7.24.0",
		"frappe-charts": "^1.5.5",
		"ionicons": "^5.2.3",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"react-use": "^14.3.0",
		"recharts": "^1.8.5",
		"rrule": "^2.6.6",
		"rxjs": "^6.6.3",
		"ts-key-enum": "^2.0.5",
		"tslib": "^2.0.3",
		"workbox-window": "^5.1.4"
	},
	"devDependencies": {
		"@types/classnames": "^2.2.11",
		"@types/node": "^14.14.14",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.6",
		"@types/recharts": "^1.8.18",
		"@types/rrule": "^2.2.9",
		"@types/webpack": "^4.41.25",
		"@types/workbox-webpack-plugin": "^5.1.5",
		"@types/workbox-window": "^4.3.3",
		"clean-webpack-plugin": "^3.0.0",
		"concurrently": "^5.3.0",
		"copy-webpack-plugin": "^5.1.2",
		"css-loader": "^3.6.0",
		"eslint-plugin-react-app": "^6.2.2",
		"eslint-plugin-react-hooks": "^3.0.0",
		"file-loader": "^6.2.0",
		"firebase-tools": "^8.20.0",
		"html-webpack-plugin": "^4.5.0",
		"ionic": "^5.4.16",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.3.0",
		"ts-loader": "^8.0.12",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.2",
		"workbox-webpack-plugin": "^5.1.4"
	},
	"description": "Phungible: Simple budgeting"
}
