{
  "name": "phungible",
  "version": "1.0.1",
  "description": "Phungible budget app",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "src/server/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build:watch": "webpack --watch",
    "build:prod": "webpack",
    "explore-bundle": "source-map-explorer ./build/js/app.js",
    "test": "npm run test:unit && npm run test:lint",
    "test:coverage": "nyc -e '.ts' --r html -r lcov -r text npm run test:unit",
    "test:unit": "TS_NODE_PROJECT=src/client TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha --compilers ts:ts-node/register,tsx:ts-node/register src/client/**/*.test.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:lint": "tslint --project tslint.json src/**/*.ts*"
  },
  "author": "David Granado",
  "license": "ISC",
  "devDependencies": {
    "@types/accounting": "^0.4.1",
    "@types/chai": "^4.2.9",
    "@types/classnames": "^2.2.3",
    "@types/localforage": "0.0.34",
    "@types/material-ui": "^0.20.7",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/react": "^16.3.12",
    "@types/react-addons-css-transition-group": "^15.0.4",
    "@types/react-dom": "^16.0.5",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "sass-loader": "^8.0.2",
    "source-map-explorer": "^1.5.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.2.5",
    "localforage": "^1.7.1",
    "localforage-cordovasqlitedriver": "^1.7.0",
    "material-ui": "^0.20.0",
    "mobx": "^3.6.2",
    "mobx-react": "^4.4.3",
    "mobx-utils": "^3.2.2",
    "moment": "^2.22.1",
    "moment-recur": "^1.0.5",
    "navigo": "^5.3.3",
